import "system1-model";

main {
  s1 isa Machine {
    web isa Service {
      state = State.stopped;
    }
  }
  s2 extends s1, {
    web.state = State.running;
  }
  pc1 isa Client {
    refer = s2.web;
  }
  pc2 := pc1;
  global {
    pc1.refer.state = State.running;
    pc2.refer.state = State.running;
  }
}
