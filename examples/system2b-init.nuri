import "system2-model";
main {
  cloud0 isa Cloud { }

  vm0 isa VM {
    in_cloud = cloud0;
    state = State.running;
    lb isa LoadBalancer {
      state = State.running;
    }
  }
  vm0_0 isa VM {
    in_cloud = cloud0;
    state = State.running;
    app isa AppService {
      state = State.running;
    }
  }
  vm0_1 extends vm0_0
  vm0_2 extends vm0_0
}
